#include<stdio.h>
int main()
{
	int i,j;
	i=1;
	j=2;
	printf("%d,%d",i,j);
	return 0;
}


#include<stdio.h>
int main()
{
	int i,j;
	i=1;
	j=2;
	printf("%d,%d",++i,++j);
	return 0;
	
}



#include<stdio.h>
int main()
{
	int i,j;
	i=1;
	j=2;
	printf("%d,%d,%d,%d",i,j,i++,j++);
	return 0;
	
}



#include<stdio.h>
int main()
{
	int i,j,a=0,b=0;
	i=1;
	j=2;
	a+=i++;
	b-=--j;
	printf("i=%d,j=%d,m=%d,n=%d",i,j,a,b);
	return 0;
	 
}





#include<stdio.h>
int main()
{
	flaot x,y;
	scanf("%f",&x);
	y=4.8*x-1/2;
	printf("%f\n",y);
	return 0;
	 
}



#include<stdio.h>
int main()
{
	int x,y;
	scanf("%d",&x);
	y=x%2/5-x;
	printf("%d\n",y);
	return 0;
	
}






#include<stdio.h>
int main()
{
	int a=8,b=1;
	a+=b+1;
	printf("a=%d\n",a);
	a/=b+1;
	return 0;
}
	
	
	
	
#include<stdio.h>
int main()
{
	char ch;
	int i;
	float f;
	double d;
	ch='A';
	i=1;
	f=2.0;
	d=2.5;
	printf("%f",ch/i+f*d-(f+i));
	return 0;
	
}


